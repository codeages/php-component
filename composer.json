{
    "name": "codeages/library",
    "description": "A php library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CodeAges",
            "email": "team@codeages.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Codeages\\Library\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Codeages\\Library\\Tests\\": "tests/"
        }
    },
    "require": {
        "monolog/monolog": "^1.20",
        "datto/json-rpc": "^6.0",
        "symfony/http-client": "^4.3",
        "guzzlehttp/guzzle": "~6.0",
        "paragonie/random_compat": ">=2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
